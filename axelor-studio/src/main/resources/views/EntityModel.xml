<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">
  
  <grid name="entity-model-grid" title="Entity models" model="com.axelor.studio.db.EntityModel">
       <field name="name"/>
       <field name="packageName"/>
  </grid>
    
  <form name="entity-model-form" title="Entity model" model="com.axelor.studio.db.EntityModel" width="large">
      <panel name="mainPanel">
      	<field name="name"/>
      	<field name="packageName"/>
      	<field name="entityXml" colSpan="12" widget="CodeEditor" />
      </panel>
  </form>

</object-views>
